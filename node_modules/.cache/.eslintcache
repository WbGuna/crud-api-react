/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/pages/NotFound/index.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/pages/Home/index.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/pages/RegisterCustomer/index.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/components/Todo/Todo.js",["135"],"import React, { useEffect, useState } from 'react'\nimport axios from 'axios'\nimport TodoForm from './Form/TodoForm'\nimport TodoList from './List/TodoList'\nimport { baseUrl } from '../../environments'\n\nfunction Todo(props) {\n\n    const URL = `${baseUrl}/todo`\n    const [tasks, setTasks] = useState([])\n\n    useEffect(() => {\n        getTasks()\n    },[])\n\n    const getTasks = () => {\n        axios.get(`${URL}`)\n        .then((response) => {\n            setTasks(response.data)\n        })\n    }\n\n    //usando apenas um função para editar\n    // const changeTask = (task, done) => {\n    //     axios.put(`${URL}/${task.id}`, { ...task, done: done})\n    //     .then((response) => {\n    //         getTasks()\n    //     })\n    // }\n\n    const editDescription = (task) => {\n        if (task.description === '') {\n            return\n        }\n\n        axios.put(`${URL}/${task.id}`, task)\n        .then((response) => {\n            getTasks()\n        })\n    }\n\n    const doneTask = (task) => {\n        axios.put(`${URL}/${task.id}`, { ...task, done: true})\n        .then((response) => {\n            getTasks()\n        })\n    }\n\n    const peddingTask = (task) => {\n        axios.put(`${URL}/${task.id}`, { ...task, done: false})\n        .then((response) => {\n            getTasks()\n        })\n    }\n\n    const registerTask = (descriptionForm) => {\n        let task = {\n            description: descriptionForm,\n            done: false\n        }\n\n        axios.post(`${URL}`, task)\n        .then((response) => {\n            getTasks()\n        })\n    }\n\n    const deleteTask = (id) => {\n        axios.delete(`${URL}/${id}`)\n        .then((response) => {\n            getTasks()\n        })\n    }\n\n    return(\n        <>\n            <TodoForm register={registerTask}/>\n            <TodoList tasks={tasks} \n                doneTask={doneTask}\n                peddingTask={peddingTask}\n                delete={deleteTask}\n                editDescription={editDescription}/>\n                {/* //usando apenas um função para editar */}\n                {/* changeTask={changeTask}/> */}\n        </>\n    )\n}\n\nexport default Todo","/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/components/template/Header/index.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/models/index.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/environments.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/components/Todo/List/TodoList.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/components/Todo/Form/TodoForm.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/models/customer.model.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exemplo10-5/crud-api-react-turma-7/src/models/tasks.model.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exercicio 10-5/crud-api-react-willian-barbosa/src/index.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exercicio 10-5/crud-api-react-willian-barbosa/src/App.js",[],"/home/wabarbosa/Área de Trabalho/Quero_ser_Dev/Atividades Jonas/exercicio 10-5/crud